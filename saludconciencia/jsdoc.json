{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "exclude": ["build"],
        "includePattern": ".js(x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "encoding": "utf8"
    }
}